{
    "name": "assisted-mindfulness/pwned-validator",
    "description": "Super simple Laravel Validator for checking password via the Pwned Passwords service of Have I Been Pwned",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephen Rees-Carter",
            "email": "stephen@rees-carter.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "AssistedMindfulness\\Pwned\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssistedMindfulness\\Pwned\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "laravel/framework": "^9.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "vimeo/psalm": "^4.3 || ^5.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench-core": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AssistedMindfulness\\Pwned\\PwnedServiceProvider"
            ]
        }
    }
}
